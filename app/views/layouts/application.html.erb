<!DOCTYPE html>
<html>
  <head>
    <title>MisNotas</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

<div>
<div>
  <% if college_signed_in? %>
	  	<%= link_to "Cerrar Sesion Colegio", destroy_college_session_path, method: :delete %> <p class="notice"><%= notice %></p>
	  <% else %>
	  	<%= link_to "Iniciar Sesion Colegio", new_college_session_path %> - 
	  	<%= link_to "Crear Colegio(elim)", new_college_registration_path %>
	  <%end%>
</div>
<br>
<div>
   <% if teacher_signed_in? %>
      <%= link_to "Cerrar Sesion Profe", destroy_teacher_session_path, method: :delete %> <p class="notice"><%= notice %></p>
    <% else %>
      <%= link_to "Iniciar Sesion Profe", new_teacher_session_path %> -
      <%= link_to "Crear Profe(elim)", new_teacher_registration_path %>
    <%end%>
</div>
<br>
<div>
   <% if student_signed_in? %>
      <%= link_to "Cerrar Sesion Estudiante", destroy_student_session_path, method: :delete %> <p class="notice"><%= notice %></p>
    <% else %>
      <%= link_to "Iniciar Sesion Estudiante", new_student_session_path %> -
      <%= link_to "Crear Estudiante(elim)", new_student_registration_path %>
    <%end%>
</div>
</div>


  <body>
    <%= yield %>
  </body>
</html>
